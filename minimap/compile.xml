<project name="MyProject" default="build" basedir=".">

	<target name="build">

		<!-- compile the Lecker-Mittag application -->
		<delete dir="bin" />
		<mkdir dir="bin" />
		<javac srcdir="src" destdir="bin" source="1.6" target="1.6" encoding="utf-8" debug="true" debuglevel="lines,vars,source" includeantruntime="false">
			<compilerarg value="-proc:none" />
		</javac>

	</target>

</project>